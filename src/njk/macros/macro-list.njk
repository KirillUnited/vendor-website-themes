{% macro gallery(items) %}
    <ul class="vw-gallery-list list-style">
        {% for item in items %}
            <li class="vw-gallery-list-item has-img-fit">
                <img src="{{item}}" alt="">
            </li>
        {% endfor %}
    </ul>
{% endmacro %}
{% macro stats(items) %}
    <ul class="list-style">
        {% for item in items %}
            <li>
                <span class="rate">{{item.value}}</span>
                <span>{{item.key}}</span>
            </li>
        {% endfor %}
    </ul>
{% endmacro %}